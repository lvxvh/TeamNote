<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.mysql.Auth" table="Auth">
        <id name="authId" type="int">
            <column name="authId" />
            <generator class="assigned" />
        </id>
        <property name="userId" type="int">
            <column name="userId"/>
        </property>
        <property name="notebookId" type="int">
            <column name="notebookId" />
        </property>
        <property name="auth" type="java.lang.String">
            <column name="auth" />
        </property>
    </class>
</hibernate-mapping>